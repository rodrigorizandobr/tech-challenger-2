        {
          "schema_version": 0,
          "attributes": {
            "acl": null,
            "arn": "arn:aws:s3:::b3-bovespa-raw-data-dev/scripts/etl_job.py",
            "bucket": "b3-bovespa-raw-data-dev",
            "bucket_key_enabled": false,
            "cache_control": "",
            "checksum_algorithm": null,
            "checksum_crc32": "",
