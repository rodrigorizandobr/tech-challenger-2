            "checksum_crc64nvme": "",
            "checksum_sha1": "",
            "checksum_sha256": "",
            "content": "import sys\nfrom awsglue.transforms import *\nfrom awsglue.utils import getResolvedOptions\nfrom pyspark.context import SparkContext\nfrom awsglue.context import GlueContext\nfrom awsglue.job import Job\nfrom pyspark.sql.functions import *\nfrom datetime import datetime\n\n# Obter argumentos do job\nargs = getResolvedOptions(sys.argv, ['JOB_NAME', 'raw-bucket', 'refined-bucket'])\n\nsc = SparkContext()\nglueContext = GlueContext(sc)\nspark = glueContext.spark_session\njob = Job(glueContext)\njob.init(args['JOB_NAME'], args)\n\n# Ler dados do bucket raw\nraw_bucket = args['raw-bucket']\nrefined_bucket = args['refined-bucket']\n\n# Ler dados do bucket raw\ndf = spark.read.parquet(f\"s3://{raw_bucket}/\")\n\n# Transformações\n# Agrupamento e sumarização\ndf_summary = df.groupBy(\"acao\").agg(\n    count(\"*\").alias(\"total_registros\"),\n    sum(\"volume\").alias(\"volume_total\"),\n    avg(\"preco\").alias(\"preco_medio\")\n)\n\n# Renomear colunas\ndf_summary = df_summary.withColumnRenamed(\"acao\", \"codigo_acao\") \\\n                      .withColumnRenamed(\"volume_total\", \"volume_negociado\")\n\n# Cálculos com datas\ndf_summary = df_summary.withColumn(\"data_processamento\", current_date()) \\\n                      .withColumn(\"hora_processamento\", current_timestamp())\n\n# Salvar no bucket refined\ndf_summary.write.partitionBy(\"data_processamento\", \"codigo_acao\") \\\n    .mode(\"overwrite\") \\\n    .parquet(f\"s3://{refined_bucket}/\")\n\njob.commit()\n",
            "content_base64": null,
